<div class="page-header">
    <h1>Listing surveys</h1>
</div>
<table class="table table-striped">
  <thead>
  <tr>
    <th>Ruler height</th>
    <th>Ruler width</th>
    <th>Ip address</th>
    <th>Response Count</th>
    <th></th>
  </tr>
  </thead>
  <tbody>
<% @surveys.each do |survey| %>
  <tr>
    <td><%= survey.ruler_height %></td>
    <td><%= survey.ruler_width %></td>
    <td><%= survey.ip_address %></td>
    <td><%= survey.responses.count %></td>
    <td><%= link_to 'Show', survey %></td>
  </tr>
<% end %>
</tbody>
</table>

<br />

<%= link_to 'New Survey', new_survey_path %>
